/*   Algorithme training on Baekjoon(https://www.acmicpc.net/)

https://www.acmicpc.net/problem/11720
*/
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {

    int n, tmp, sum=0;
    char str[100];
    scanf("%d\n",&n);
    scanf("%s\n",&str);

    for(int i=0; i<n; i++){
        sum+= str[i]-48;

    }
    printf("%d",sum);


    return 0;
}
